a googleLink(href, name, target, . . . ) extends: Link{
        attrs{
            href = "https://www.google.com";
            name = "theLink"
            target = "_blank";
        }

        inner{
            Click here!
        }
        return a
    }

h1 header() extends: bodyText{
    inner{
        Hello @{name}!
    }
}